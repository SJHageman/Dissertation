@article{zurchDirectSimultaneousObservation2017,
  title = {Direct and Simultaneous Observation of Ultrafast Electron and Hole Dynamics in Germanium},
  author = {Z{\"u}rch, Michael and Chang, Hung-Tzu and Borja, Lauren J. and Kraus, Peter M. and Cushing, Scott K. and Gandman, Andrey and Kaplan, Christopher J. and Oh, Myoung Hwan and Prell, James S. and Prendergast, David and Pemmaraju, Chaitanya D. and Neumark, Daniel M. and Leone, Stephen R.},
