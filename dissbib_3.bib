@article{schultzeAttosecondBandgapDynamics2014,
  title = {Attosecond Band-Gap Dynamics in Silicon},
  author = {Schultze, Martin and Ramasesha, Krupa and Pemmaraju, C. D. and Sato, S. A. and Whitmore, D. and Gandman, A. and Prell, James S. and Borja, L. J. and Prendergast, D. and Yabana, K. and Neumark, Daniel M. and Leone, Stephen R.},
