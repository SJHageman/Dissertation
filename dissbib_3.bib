  file = {C\:\\Users\\Tebe\\Zotero\\storage\\JFEZ43DN\\Attar et al. - 2017 - Femtosecond x-ray spectroscopy of an electrocyclic.pdf;C\:\\Users\\Tebe\\Zotero\\storage\\5ZBZSJPN\\54.html},
