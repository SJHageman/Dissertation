  file = {C\:\\Users\\Tebe\\Zotero\\storage\\EGGZZ7QF\\Beutler - 1935 - Über Absorptionsserien von Argon, Krypton und Xeno.pdf},
  journal = {Zeitschrift f\"ur Physik},
  keywords = {Fano,ground state},
  language = {de},
  number = {3}
}

